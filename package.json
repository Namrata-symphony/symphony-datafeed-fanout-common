{
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  },
  "name": "fanout-common",
  "description": "Common code between the ecs-fanout and the lambda-fanout",
  "version": "1.0.0",
  "author": "Symphony",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "keywords": [],
  "homepage": "https://github.com/SymphonyOSF/symphony-datafeed-fanout-common",
  "scripts": {
    "build": "babel src -d dist --source-maps 'inline'",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "prepare": "npm run rebuild",
    "lint": "eslint --ext .js src",
    "test": "nyc mocha --require @babel/register --recursive spec/unit/*.spec.js --reporter spec",
    "security-check": "npm audit --registry=https://registry.npmjs.org --production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run security-check"
    }
  },
  "dependencies": {
    "lodash": "4.17.20",
    "symphony-datafeed-core": "3.2.7"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/cli": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/register": "7.12.13",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-rewire": "1.2.0",
    "chai": "4.3.0",
    "chai-as-promised": "7.1.1",
    "eslint": "7.19.0",
    "eslint-config-airbnb-base": "14.2.1",
    "mocha": "8.2.1",
    "mochawesome": "6.1.1",
    "nyc": "15.1.0",
    "sinon": "9.2.4",
    "sinon-chai": "3.5.0",
    "husky": "5.0.9"
  },
  "nyc": {
    "all": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
