{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "12"
            }
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "istanbul",
        "rewire"
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "12"
            }
          }
        ]
      ]
    }
  }
}
